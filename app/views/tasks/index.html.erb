<h1>All the tasks of one concrete project</h1>
<h2>Project title: <%= @project.title%>  </h2>
<% @tasks.each do |task| %>
    <p>
        <strong>Task title: </strong><%= task.title%>   
        <strong>Task description: </strong><%= task.description %> 
        <strong>Task status: </strong><%= task.is_done %>
    </p>
<% end %>
